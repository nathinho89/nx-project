{
  "name": "@my-app/type-utils",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "dist/packages/type-utils/index.d.ts",
  "files": ["dist", "src"],
  "scripts": {
    "build": "nx run type-utils:build",
    "dev": "tsc -b -w"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
        "options": {
          "command": "tsc -p packages/type-utils/tsconfig.build.json"
        }
      }
    }
  }
}
